{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    }
  ],
  "aliveStatusCodes": [200, 206, 999],
  "retryOn429": true,
  "retryCount": 3,
  "fallbackHttpStatus": {
    "400": "warn",
    "401": "warn",
    "403": "warn",
    "404": "error",
    "405": "warn",
    "500": "error"
  }
}
